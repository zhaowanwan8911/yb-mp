.user-wrap {
  display: flex;
  flex-direction: row;
  align-items: center;
  justify-content: space-between;
  height: 160rpx;
  padding: 0 30rpx;
  margin-bottom: 30rpx;
  border-bottom: 1rpx solid #E0E6ED;
  border-top: 1rpx solid #E0E6ED;
  background: #fff;
}
.user-wrap .left-wrap .image-wrap {
  border-radius: 120rpx;
  height: 120rpx;
  width: 120rpx;
}
.user-wrap .right-wrap {
  height: 120rpx;
  line-height: 120rpx;
}
.contact-btn {
  height: 80rpx;
  width: 100%;
  display: flex;
  flex-direction: row;
  padding: 0 30rpx;
  align-items: center;
  justify-content: space-between;
  background: #fff;
  border-bottom: 1rpx solid #e0e6ed;
}
.contact-btn::after {
  border: none;
  border-radius: 0;
}
.contact-btn .left-wrap {
  font-size: 35rpx;   
  flex: 1;
  text-align: left;
}
.contact-btn .right-wrap {
  flex: 1;
  text-align: right;
}

.contact-btn .right-wrap .image-wrap {
  width: 20rpx;
  height: 30rpx;
}