.photo-input-ctn {
  height: 160rpx;
  width: 100%;
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-bottom: 1rpx solid #E0E6ED;
  padding-left: 30rpx;
}
.left-wrap {
  flex: 1;
}
.right-wrap {
  flex: 1;
  text-align: right;
  margin-right: 60rpx;
}

.photo-wrap {
  border-radius: 60px;
  width: 60px;
  height: 60px;
}